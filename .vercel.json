{
    "redirects": [
      {
        "source": "/login",
        "destination": "https://ukkikki.xyz/login",
        "permanent": true
      },
      {
        "source": "/join",
        "destination": "https://ukkikki.xyz/join",
        "permanent": true
      },
      {
        "source": "/logout",
        "destination": "https://ukkikki.xyz/logout",
        "permanent": true
      },
      {
        "source": "/api/:path*",
        "destination": "https://ukkikki.xyz/api/:path*",
        "permanent": true
      }
    ],
    "headers": [
      {
        "source": "/(.*)",
        "headers": [
          {
            "key": "Access-Control-Allow-Origin",
            "value": "*"
          },
          {
            "key": "Access-Control-Allow-Methods",
            "value": "GET, POST, PUT, DELETE, OPTIONS"
          },
          {
            "key": "Access-Control-Allow-Headers",
            "value": "X-Requested-With, Content-Type, Authorization"
          }
        ]
      }
    ]
  }
  